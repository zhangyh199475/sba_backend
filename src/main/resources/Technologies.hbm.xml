<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ibm.apt.sba_back.domain.Technologies" table="technologies" schema="sba">
        <id name="techId">
            <column name="TECH_ID" sql-type="int"/>
        </id>
        <property name="techName">
            <column name="TECH_NAME" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="type">
            <column name="TYPE" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="active">
            <column name="ACTIVE" sql-type="int"/>
        </property>
        <property name="price">
            <column name="PRICE" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>